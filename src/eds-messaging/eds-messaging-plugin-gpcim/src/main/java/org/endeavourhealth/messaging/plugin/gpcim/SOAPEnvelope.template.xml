<soap:Envelope xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/">
    <soap:Header>
        <Action xmlns="http://www.w3.org/2005/08/addressing">{{action}}</Action>
        <MessageID xmlns="http://www.w3.org/2005/08/addressing">{{message-id}}</MessageID>
        <RelatesTo xmlns="http://www.w3.org/2005/08/addressing">{{relates-to}}</RelatesTo>
        <To xmlns="http://www.w3.org/2005/08/addressing" />
        <ReplyTo xmlns="http://www.w3.org/2005/08/addressing">
            <Address>https://emisweb/MiG</Address>
        </ReplyTo>
    </soap:Header>
    <soap:Body>
        <soap:Fault>
            <faultcode>soap:Server</faultcode>
            <faultstring>Request is invalid</faultstring>
            <detail>
                <q1:migFault xmlns:q1="http://www.healthcaregateway.co.uk/mig">
                    <q1:serviceDefinition>
                        <q1:name>Unknown</q1:name>
                        <q1:version>Unknown</q1:version>
                    </q1:serviceDefinition>
                    <q1:error>
                        <q1:category>FATAL</q1:category>
                        <q1:text>General Error</q1:text>
                    </q1:error>
                </q1:migFault>
            </detail>
        </soap:Fault>
    </soap:Body>
</soap:Envelope>