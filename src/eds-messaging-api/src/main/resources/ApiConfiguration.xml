<?xml version="1.0" encoding="UTF-8"?>
<ApiConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="../../../../eds-messaging-core/src/main/resources/ApiConfiguration.xsd">
    <GetData>
        <Pipeline>
            <ValidateSender/>
            <ValidateMessageType/>
            <PostMessageToLog/>
            <PostMessageToQueue>
                <Credentials>
                    <Username>username</Username>
                    <Password>password</Password>
                </Credentials>
                <Nodes>192.168.0.1:1001, 192.168.0.2:1002</Nodes>
                <Exchange>EdsInbound</Exchange>
            </PostMessageToQueue>
        </Pipeline>
    </GetData>
    <GetDataAsync>
        <Pipeline>
            <ValidateMessageType/>
        </Pipeline>
    </GetDataAsync>
    <PostMessage>
        <Pipeline>
            <ValidateMessageType/>
        </Pipeline>
    </PostMessage>
    <PostMessageAsync>
        <Pipeline>
            <ValidateMessageType/>
        </Pipeline>
    </PostMessageAsync>
</ApiConfiguration>