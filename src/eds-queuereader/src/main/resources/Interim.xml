<?xml version="1.0" encoding="UTF-8"?>
<QueueReaderConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../eds-messaging-core/src/main/resources/QueueReaderConfiguration.xsd">
    <Credentials>
        <Username>username</Username>
        <Password>password</Password>
    </Credentials>
    <Nodes>192.168.0.1:1000, 192.168.0.2:2000</Nodes>
    <Exchange>Interim</Exchange>
    <Queue>Interim-Orgs1000-2000</Queue>
    <RoutingKeys>Org1,Org2,Org3</RoutingKeys>
    <Pipeline>
        <RunDataDistributionProtocols/>
        <MessageTransform>
            <ExchangeProperties>
                <Property Key="TransformFrom">OpenHR</Property>
                <Property Key="TransformTo">OpenHR</Property>
            </ExchangeProperties>
        </MessageTransform>
        <PostMessageToLog/>
        <PostMessageToQueue>
            <Credentials>
                <Username>username</Username>
                <Password>password</Password>
            </Credentials>
            <Nodes>192.168.0.1:1001, 192.168.0.2:1002</Nodes>
            <Exchange>EdsResponse</Exchange>
        </PostMessageToQueue>
    </Pipeline>
</QueueReaderConfiguration>