<?xml version="1.0" encoding="UTF-8"?>
<SftpReaderConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                         xsi:noNamespaceSchemaLocation="../../../../eds-messaging-core/src/main/resources/SftpReaderConfiguration.xsd">
    <SftpReaderIdentifier>EMIS001</SftpReaderIdentifier>
    <PostgresConnetion>
        <Hostname>localhost</Hostname>
        <Port>5432</Port>
        <Database>sftpreader</Database>
        <Username>postgres</Username>
        <Password></Password>
    </PostgresConnetion>

    <!-- move to database -->
    <SftpCredentials>
        <Username>test-endeavour</Username>
        <ClientPrivateKeyFilePath>test-ssh-client-private.key</ClientPrivateKeyFilePath>
        <ClientPrivateKeyPassword></ClientPrivateKeyPassword>
        <HostPublicKeyFilePath>test-ssh-host-public.pub</HostPublicKeyFilePath>
    </SftpCredentials>
    <Host>endeavour-sftp.cloudapp.net</Host>
    <Port>22</Port>
    <RemotePath>/gpg</RemotePath>
    <LocalPath>/Users/jonny/Code/Local/sftp</LocalPath>
    <PgpDecryption>
        <PgpFileExtension>.gpg</PgpFileExtension>
        <RecipientPrivateKeyFilePath>test-pgp-endeavour-private.asc</RecipientPrivateKeyFilePath>
        <RecipientPrivateKeyPassword>password</RecipientPrivateKeyPassword>
        <SenderPublicKeyFilePath>test-pgp-emis-public.asc</SenderPublicKeyFilePath>
    </PgpDecryption>
    <PollDelaySeconds>10</PollDelaySeconds>
    <Pipeline>
        <EnvelopMessage>
            <ExchangeHeaders>
                <Header Key="content-type">text/json</Header>
                <Header Key="Sender">ODS00000</Header>
                <Header Key="SourceSystem">EDS_FTP_Poller</Header>
                <Header Key="ResponseUri">http://EDS_POLLER_BusinessAck.co.uk</Header>
                <Header Key="DestinationAddress">192.168.0.1, 192.168.0.2, 192.168.0.3</Header>
            </ExchangeHeaders>
        </EnvelopMessage>
        <PostToRest>
            <ExchangeHeaders>
                <Header Key="DestinationAddress">http://localhost:8080/api/PostMessageAsync</Header>
            </ExchangeHeaders>
            <SendHeaders>Content-Type</SendHeaders>
        </PostToRest>
    </Pipeline>
    <!-- -->
</SftpReaderConfiguration>