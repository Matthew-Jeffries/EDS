<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="Pipeline.xsd"/>

    <xs:element name="ApiConfiguration">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="GetData">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Pipeline" type="Pipeline" minOccurs="1" maxOccurs="1"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="GetDataAsync">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Pipeline" type="Pipeline" minOccurs="1" maxOccurs="1"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="PostMessage">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Pipeline" type="Pipeline" minOccurs="1" maxOccurs="1"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="PostMessageAsync">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Pipeline" type="Pipeline" minOccurs="1" maxOccurs="1"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
